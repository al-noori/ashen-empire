<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
      prefHeight="400.0" prefWidth="600.0" spacing="50.0" xmlns="http://javafx.com/javafx/21.0.1"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uniks.stp24.controller.AchievementsController">
    <TableView fx:id="table" styleClass="primaryTableView" prefHeight="200.0" prefWidth="200.0">
        <placeholder>
            <Label/>
        </placeholder>
      <columns>
          <TableColumn fx:id="nameColumn" prefWidth="75.0" text="%name"/>
          <TableColumn fx:id="descriptionColumn" prefWidth="131.0" text="%description"/>
          <TableColumn fx:id="progressColumn" prefWidth="131.0" text="%progress"/>
          <TableColumn fx:id="unlockedAtColumn" prefWidth="131.0" text="%unlocked.at"/>
      </columns>
        <columnResizePolicy>
          <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
        <VBox.margin>
          <Insets left="50.0" right="50.0"/>
        </VBox.margin>
    </TableView>
    <Button fx:id="backButton" mnemonicParsing="false" onMouseClicked="#backButtonClicked" text="%back"
            styleClass="primary-button">
        <font>
          <Font size="24.0"/>
        </font>
    </Button>
</VBox>
