<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<StackPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uniks.stp24.controller.ChooseEmpireController">
    <VBox alignment="CENTER" spacing="50.0">
      <ListView fx:id="empiresBox" styleClass="primary-list-view" />
        <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="100.0">
            <Button mnemonicParsing="false" onMouseClicked="#backButton" styleClass="primary-button" text="%back"/>
            <Button fx:id="saveButton" mnemonicParsing="false" onMouseClicked="#saveButton" styleClass="primary-button" text="%save"/>
        </HBox>
    </VBox>
</StackPane>
