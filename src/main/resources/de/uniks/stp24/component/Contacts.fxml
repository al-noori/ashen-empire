<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<fx:root type="AnchorPane" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uniks.stp24.component.ContactsComponent">

<BorderPane fx:id="borderPane" minHeight="800.0" minWidth="1200.0" styleClass="contacts" AnchorPane.bottomAnchor="200.0" AnchorPane.leftAnchor="350.0" AnchorPane.rightAnchor="350.0" AnchorPane.topAnchor="200.0">
      <left>
         <VBox alignment="CENTER_LEFT" spacing="30.0" BorderPane.alignment="CENTER">
            <padding>
               <Insets bottom="20.0" left="20.0" right="20.0" />
            </padding>
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0" spacing="20.0">
               <VBox styleClass="imageBackground">
                  <children>
                     <ImageView fx:id="CharacterImage" fitHeight="200.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" />
                  </children>
               </VBox>
               <VBox maxHeight="120.0" prefHeight="120.0" styleClass="imageBackground">
                  <children>
                     <ImageView fx:id="EmpireImage" fitHeight="120.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true" />
                  </children>
               </VBox>
            </HBox>
            <Text fx:id="EmpireText" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="largeText" text="Empire No. XX" />
            <HBox alignment="CENTER_LEFT">
               <ImageView fx:id="EconomyImage" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" />
               <Text fx:id="EconomyText" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="mediumText" text="Economy: XX">
                  <HBox.margin>
                     <Insets left="20.0" />
                  </HBox.margin></Text>
            </HBox>
            <HBox alignment="CENTER_LEFT">
               <ImageView fx:id="MiliaryImage" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" />
               <Text fx:id="MilitaryText" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="mediumText" text="Military: XX">
                  <HBox.margin>
                     <Insets left="20.0" />
                  </HBox.margin></Text>
            </HBox>
            <HBox alignment="CENTER_LEFT">
               <ImageView fx:id="TechImage" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" />
               <Text fx:id="TechText" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="mediumText" text="Tech: XX">
                  <HBox.margin>
                     <Insets left="20.0" />
                  </HBox.margin></Text>
            </HBox>
         </VBox>
      </left>
      <center>
         <VBox spacing="30.0" BorderPane.alignment="CENTER">
            <padding>
               <Insets bottom="20.0" right="20.0" />
            </padding>
            <HBox alignment="CENTER" prefHeight="100.0" styleClass="PopUpsBackground">
               <VBox alignment="CENTER" styleClass="PopUpsBackground">
                  <children>
                     <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="mediumText" text="Name" />
                  </children>
                  <padding>
                     <Insets left="150.0" right="160.0" />
                  </padding>
               </VBox>
               <VBox alignment="CENTER" styleClass="PopUpsBackground">
                  <children>
                     <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="mediumText" text="Power" />
                  </children>
                  <padding>
                     <Insets left="100.0" right="100.0" />
                  </padding>
               </VBox>
               <VBox alignment="CENTER" styleClass="PopUpsBackground">
                  <children>
                     <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="mediumText" text="War" />
                  </children>
                  <padding>
                     <Insets left="90.0" right="95.0" />
                  </padding>
               </VBox>
            </HBox>
            <HBox fx:id="empiresBox" styleClass="PopUpsBackground" />
            <HBox alignment="CENTER">
               <padding>
                  <Insets top="20.0" />
               </padding>
               <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="200.0">
                  <VBox alignment="CENTER" styleClass="PopUpsBackground">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="mediumText" text="Players" />
                     </children>
                  </VBox>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="verybigText" text="&gt;&gt;">
                     <VBox.margin>
                        <Insets top="20.0" />
                     </VBox.margin>
                  </Text>
                  <padding>
                     <Insets right="30.0" />
                  </padding>
               </VBox>
               <TableView fx:id="playersBox" styleClass="secondaryTableView">
                 <columns>
                   <TableColumn fx:id="picTableColumn" maxWidth="-1.0" minWidth="70.0" prefWidth="-1.0" />
                   <TableColumn fx:id="nameTableColumn" maxWidth="-1.0" minWidth="300.0" prefWidth="-1.0" />
                 </columns>
               </TableView>
            </HBox>
         </VBox>
      </center>
      <bottom>
         <HBox alignment="CENTER_LEFT" spacing="50.0" BorderPane.alignment="CENTER_LEFT">
            <Button fx:id="BackButton" mnemonicParsing="false" onMouseClicked="#onBackButton" styleClass="primary-button" text="Back" />
            <Button fx:id="DiplomacyButton" mnemonicParsing="false" onMouseClicked="#onDiplomacyButton" styleClass="primary-button" text="Diplomacy" />
            <padding>
               <Insets top="50.0" />
            </padding>
            <BorderPane.margin>
               <Insets bottom="20.0" left="20.0" />
            </BorderPane.margin>
         </HBox>
      </bottom>
      <top>
         <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="largeText" text="Contacts" BorderPane.alignment="CENTER_LEFT">
            <BorderPane.margin>
               <Insets left="20.0" top="20.0" />
            </BorderPane.margin>
         </Text>
      </top></BorderPane>

</fx:root>
