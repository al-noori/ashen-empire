<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<fx:root maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" styleClass="fleetDetails" type="VBox" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.uniks.stp24.component.TankTransferComponent">
    <VBox>
        <Text fx:id="tankTransferName" fill="#ffd52c" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="mediumText" text="TANK TRANSFER">
         <VBox.margin>
            <Insets left="10.0" top="10.0" />
         </VBox.margin></Text>
      <ListView fx:id="tankList" prefHeight="282.0" prefWidth="248.0" styleClass="systemListView">
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="5.0" />
         </VBox.margin></ListView>
      <Text fill="#ffd52c" layoutX="20.0" layoutY="33.0" strokeType="OUTSIDE" strokeWidth="0.0" text="To Division:">
         <VBox.margin>
            <Insets left="10.0" right="10.0" top="5.0" />
         </VBox.margin>
      </Text>
      <ComboBox fx:id="selectDivisionComboBox" onAction="#selectDivision" prefHeight="25.0" prefWidth="250.0">
         <VBox.margin>
            <Insets left="10.0" right="10.0" />
         </VBox.margin>
      </ComboBox>
        <VBox alignment="TOP_CENTER" prefHeight="8.0" prefWidth="268.0">
            <VBox.margin>
                <Insets top="10.0" />
            </VBox.margin>
        </VBox>
        <HBox alignment="TOP_CENTER" spacing="20.0">
            <Button fx:id="cancelButton" onAction="#cancel" styleClass="primary-button" text="Cancel" />
            <Button fx:id="transferButton" onAction="#transfer" styleClass="primary-button" text="Transfer" />
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <VBox.margin>
            <Insets bottom="10.0" top="10.0" />
         </VBox.margin>
        </HBox>
    </VBox>
</fx:root>
